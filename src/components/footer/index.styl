.footer_warp
  border-top 1px solid #333
  position fixed
  z-index 100
  left 0
  right 0
  bottom 0
  width 100%
  height 55px
  display flex
  background #fff
  justify-content space-around
  .on
    color red
  .footer_item
    display flex
    flex 1
    text-align center
    flex-direction column
    align-items center
    margin 5px
    span
      font-size 12px
      margin-top 2px
      margin-bottom 2px
      .iconfont
        font-size 20px
