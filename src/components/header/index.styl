.header_warp
  width 100%
  position fixed
  background #fff
  z-index 100
  left 0
  top 0
  .home_header
    width 100%
    height 50px
    display flex
    align-items center
    margin-left 10px
    img
      width 1.84rem
      height 0.53rem
    .home_header_sou
      flex-direction column
      align-items center
      text-align center
      width 220px
      border-radius 7px
      height 30px
      line-height 30px
      justify-content center
      background #ededed
      margin-left 10px
    .home_header_deng
      text-align center
      line-height 20px
      width 40px
      height 20px
      border 1px solid red
      margin-left 10px
      border-radius 7px
  .home_nav
    position relative
    height 22px
    width 500px
    .home_list
      // width 500px
      height 22px
      white-space nowrap
      display flex
      justify-content space-around
      a
        margin-left 20px
      .home_item
        font-size 15px
        color #7f7f7f
        text-decoration none
      .xuan
        color red
        border-bottom 2px solid red
  .home_icon
    position fixed
    text-align center
    line-height 20px
    background #fff
    right 0
    top 50px
    width 50px
    height 20px
